{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "Node",
    "importsNotUsedAsValues": "remove",
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  }
}
